module Compile

menus
  
  menu: "Compile" (openeditor)
    
    action: "Show WebAssembly AST"   = show-wast-ast    (source)
    action: "Compile to .wast"       = compile-to-wast  (source)
    action: "Compile to .wasm"       = compile-to-wasm  (source)
    action: "Run"                    = run-wasm         (source)

views
  
  outline view: editor-outline (source)
    expand to level: 3
