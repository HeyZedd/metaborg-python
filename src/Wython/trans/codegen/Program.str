module Program

imports
  signatures/WebAssembly/-
  trans/codegen/-
  ast-syntax/-

strategies
  codegen-all = codegen
 
rules
  codegen: Module(stmts) -> WastModule(iList, dList, fList, eList)
    where
       <debug> stmts;
       iList := <imports> stmts;
       dList := <data> stmts;
       fList := <funcs> stmts;
       eList :=  <exports> stmts
       