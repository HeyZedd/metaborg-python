module Imports

imports
  src-gen/signatures/WebAssembly/WastFunc-sig
  src-gen/signatures/WebAssembly/WastImport-sig
  trans/codegen/-
  ast-syntax/AST-Wython
  ast-syntax/AST-Statement

rules

  imports:
    _ -> [ 
      WastFunc(
        Some("$a"),
        [WastImport("\"lib\"", "\"logString\""),
        Some(WastParams([I32(), I32()]))]
      ),
      WastMem(Some("$d"), WastImport("\"memory\"", "\"memory\""), "1")
    ]