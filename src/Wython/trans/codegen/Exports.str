module Program

imports
  Module-sig
  trans/codegen/-
  ast-syntax/AST-Wython
  ast-syntax/AST-Statement

strategies
  codegen-all = codegen
 
rules
  codegen: Module(stmts) -> Module(iList, dList, fList, eList)
    where
       iList := <imports> stmts;
       dList := <data> stmts;
       fList := <funcs> stmts;
       eList :=  <exports> stmts
       